<h1>Articles</h1>

<div id="articles">
  <% @articles.each do |article| %>
    <div id="<%= dom_id(article) %>" class="mb-2">
      <%= render 'article_in_list', article: %>
    </div>
  <% end %>
</div>

<hr />

<%= turbo_frame_tag 'article_wrapper' do %>
  <%= link_to "New article", new_article_path %>
<% end %>
