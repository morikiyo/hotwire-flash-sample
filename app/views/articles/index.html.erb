<p style="color: green"><%= notice %></p>

<h1>Articles</h1>

<div id="articles">
  <% @articles.each do |article| %>
    <%= render article %>
    <p>
      <%= link_to "Show this article", article, data: { turbo_frame: modal_frame_id } %>
    </p>
  <% end %>
</div>

<%= link_to "New article", new_article_path, data: { turbo_frame: modal_frame_id } %>
